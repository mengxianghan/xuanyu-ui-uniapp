page {
  font-size: 28rpx;
  font-family: 'PingFang SC';
}
view,
scroll-view,
swiper,
button,
input,
textarea,
label,
navigator,
image {
  box-sizing: border-box;
  outline: none;
}
.x-hairline,
.x-hairline--top,
.x-hairline--left,
.x-hairline--right,
.x-hairline--bottom,
.x-hairline--top-bottom,
.x-hairline--surround {
  position: relative;
}
.x-hairline::after,
  .x-hairline--top::after,
  .x-hairline--left::after,
  .x-hairline--right::after,
  .x-hairline--bottom::after,
  .x-hairline--top-bottom::after,
  .x-hairline--surround::after {
    position: absolute;
    box-sizing: border-box;
    -webkit-transform-origin: center;
            transform-origin: center;
    /* cover wechat button:after default transforn-origin */
    content: ' ';
    pointer-events: none;
    top: -50%;
    right: -50%;
    bottom: -50%;
    left: -50%;
    border: 0 solid #eee;
    -webkit-transform: scale(0.5);
            transform: scale(0.5);
}
.x-hairline--top::after {
  border-top-width: 1px;
}
.x-hairline--left::after {
  border-left-width: 1px;
}
.x-hairline--right::after {
  border-right-width: 1px;
}
.x-hairline--bottom::after {
  border-bottom-width: 1px;
}
.x-hairline--top-bottom::after {
  border-width: 1px 0;
}
.x-hairline--surround::after {
  border-width: 1px;
}
