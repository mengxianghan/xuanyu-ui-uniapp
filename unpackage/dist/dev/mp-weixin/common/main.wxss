uni-button[plain] {
  border-color: currentColor;
}
uni-button:after {
  border-radius: inherit;
}
.x-button {
  position: relative;
  display: -webkit-inline-box;
  display: -webkit-inline-flex;
  display: inline-flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  box-sizing: border-box;
  vertical-align: middle;
  -webkit-appearance: none;
  -webkit-text-size-adjust: 100%;
  height: 44px;
  font-size: 16px;
  -webkit-transition: opacity 0.2s;
  transition: opacity 0.2s;
  border-radius: 2px;
  padding: 0 32rpx;
  font-size: 14px;
}
.x-button::before {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 100%;
    height: 100%;
    border: inherit;
    border-radius: inherit;
    /* inherit parent's border radius */
    -webkit-transform: translate(-50%, -50%);
            transform: translate(-50%, -50%);
    opacity: 0;
    content: ' ';
    background-color: #000;
    border-color: #000;
}
.x-button::after {
    border-width: 0;
}
.x-button--default {
    color: #323233;
    background-color: #fff;
    border: 1px solid #ebedf0;
}
.x-button--primary {
    color: #fff;
    background-color: #07c160;
    border: 1px solid #07c160;
}
.x-button--info {
    color: #fff;
    background-color: #1989fa;
    border: 1px solid #1989fa;
}
.x-button--danger {
    color: #fff;
    background-color: #ee0a24;
    border: 1px solid #ee0a24;
}
.x-button--warning {
    color: #fff;
    background-color: #ff976a;
    border: 1px solid #ff976a;
}
.x-button--plain {
    background-color: #fff;
}
.x-button--plain.x-button--primary {
      color: #07c160;
}
.x-button--plain.x-button--info {
      color: #1989fa;
}
.x-button--plain.x-button--danger {
      color: #ee0a24;
}
.x-button--plain.x-button--warning {
      color: #ff976a;
}
.x-button--large {
    height: 50px;
}
.x-button--small {
    min-width: 60px;
    height: 30px;
    padding: 0 8px;
    font-size: 12px;
}
.x-button--mini {
    min-width: 50px;
    height: 22px;
    font-size: 10px;
    padding-left: 8rpx;
    padding-right: 8rpx;
}
.x-button--block {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    width: 100%;
}
.x-button--round {
    border-radius: 999px;
}
.x-button--square {
    border-radius: 0;
}
.x-button--disabled {
    opacity: 0.5;
}
.x-button--disabled.x-button--primary {
      color: #fff !important;
